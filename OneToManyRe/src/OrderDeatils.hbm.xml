<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="lk.ijse.OneToMany.entity.OrderDeatils" table="OrderDetails">
        <id name="code" column="orderDetails_code">
            <generator class="lk.ijse.OneToMany.Generator.OrderDetailsIDGenerator"/>
        </id>
        <property name="itemCode" column="itemCode"></property>
        <property name="unitPrice" column="unitPrice"/>
        <property name="qty" column="qty"/>
    </class>
</hibernate-mapping>